{
  "private": true,
  "name": "imagemin-min",
  "version": "0.4.0-pre",
  "description": "Like imagemin but batteries-included",
  "author": "dr-js",
  "license": "MIT",
  "repository": "github:dr-js/imagemin-min",
  "main": "./library/index.js",
  "bin": {
    "imagemin-min": "bin/min-in-place.js",
    "imagemin-min-in-place": "bin/min-in-place.js",
    "imagemin-min-test-boot": "bin/test-boot.js"
  },
  "scripts": {
    "// repo ========================": "",
    "version-bump": "dr-dev --version-bump-git-branch --git-commit --debug",
    "tag-push": "dr-dev --version-bump-push-check --git-commit",
    "// script ======================": "",
    "script-pack": "node ./script/pack.js pack",
    "script-pack-test": "npm run script-pack -- verbose test",
    "script-publish": "npm run script-pack -- publish",
    "script-generate-spec": "node ./script/generateSpec.js",
    "script-generate-battery-svgo": "node ./script/generateBatterySVGO.js",
    "// build =======================": "",
    "build-library": "babel ./source --out-dir ./output-gitignore/library --copy-files",
    "build-library-function": "node ./script/webpack.js production profile",
    "build-library-function-dev": "node ./script/webpack.js development watch",
    "// test ========================": "",
    "test-dev": "dr-dev --EE BABEL_ENV=dev -E -- dr-dev --TR @babel/register -T source/battery/svgo.test.js",
    "test-dev-sharp": "dr-dev --EE BABEL_ENV=dev -E -- dr-dev --TR @babel/register -T source/battery/sharp.test.js",
    "test-source": "dr-dev --TR @babel/register -T source/ --TFS .test.js",
    "test-output": "dr-dev -T output-gitignore/ --TFS .test.js",
    "// =============================": "",
    "test": "npm run script-pack-test",
    "lint": "eslint ./source/ ./script/",
    "prepack": "echo \"Error: pack with script-*\" && exit 1"
  },
  "engines": {
    "node": ">=18.12",
    "npm": ">=8.19"
  },
  "peerDependencies": {
    "@min-pack/sharp": "^0.34.5"
  },
  "devDependencies": {
    "@dr-js/core": "^0.5.17",
    "@dr-js/dev": "^0.5.14",
    "@dr-js/dev-web": "^0.5.14",
    "svgo": "4.0.0"
  }
}
